
<h1>Team TB Jobs</h1>
<% @jobs.each do |job| %>
  <h3><%= job.title %></h3>
  <h3><%= job.pay %></h3>
  <h3><%= job.location %></h3>
  <p><%= job.description %></p>
  <h5><%= link_to "Edit Posting", edit_job_path(job)%></h5>
  <h5><%= link_to 'Delete Posting', job, method: :delete %></h5>
<% end %>